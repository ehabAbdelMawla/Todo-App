        "@testing-library/user-event": "^12.7.1",
        "aes256": "^1.1.0",
        "chokidar": "^3.5.1",
        "cross-env": "^7.0.3",
        "custom-react-electron-titlebar": "^1.0.6",
        "electron-is-dev": "^1.2.0",
        "firebase": "^8.2.7",
        "firebase-admin": "^9.5.0",
        "firebase-functions": "^3.13.1",
        "jquery": "^3.5.1",
        "jquery.nicescroll": "^3.7.6",
        "node-sass": "^5.0.0",
        "react": "^17.0.1",
        "react-awesome-modal": "^2.0.5",
